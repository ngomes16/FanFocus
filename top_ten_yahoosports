import requests
from bs4 import BeautifulSoup

# URL of the Yahoo Sports page for the Golden State Warriors
url = "https://sports.yahoo.com/nba/teams/golden-state/"

# Send a GET request to the webpage
response = requests.get(url)
soup = BeautifulSoup(response.text, 'html.parser')

# Find all article links in the targeted section
articles_section = soup.find('ul', class_='My(0) P(0) Wow(bw) Ov(h)')  # Adjusted target to the broader <ul> section
if articles_section:
    articles = articles_section.find_all('a', href=True, limit=10)  # Find all <a> tags with href attribute
    links = [article['href'] for article in articles if article['href'].startswith('http')]
    
    # Print the links
    for idx, link in enumerate(links, 1):
        print(f"Article {idx}: {link}")
else:
    print("Could not find the targeted article section.")
